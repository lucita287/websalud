    <description></description>
    <display-name>SUploadImagen</display-name>
    <servlet-name>SUploadImagen</servlet-name>
    <servlet-class>servlet.SUploadImagen</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SUploadImagen</servlet-name>
    <url-pattern>/uploadimagen</url-pattern>
